{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "types": ["jest"],
    "typeRoots": ["node_modules/@types", "src/types"],
    "baseUrl": "./",
    "paths":{
      "@assets/*": ["assets/*"],
      "@components/*": ["src/components/*"],
      "@features/*": ["src/features/*"],
      "@service/*": ["src/service/*"],
      "@state/*": ["src/state/*"],
      "@utils/*": ["src/utils/*"],
      "@navigation/*": ["src/navigation/*"],
    }
  }
}
